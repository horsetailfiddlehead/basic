	name = PatrickMa
	email = patrick.is@not.here
[color]
	diff = auto
	status = auto
	branch = auto
	interactive= true
	ui = true
[core]
	excludesfile = ~/.gitignore_global
	ignorecase = true
	autocrlf = input
	hookspath = ~/.githooks
[push]
	default = upstream
[credential]
	helper = /cygdrive/C/Users/Patrick/AppData/Roaming/GitCredStore/git-credential-winstore.exe
[alias]
	tree = log --graph --oneline --decorate --color
	treeall = log --graph --oneline --decorate --all --color
	last = log -1 --pretty=medium
	root = rev-parse --show-toplevel
	skipped = !git ls-files -v | grep ^[[:lower:]S]
	skip = update-index --skip-worktree
	unskip = update-index --no-skip-worktree
	hide = update-index --assume-unchanged
	unhide = update-index --no-assume-unchanged
	alias = ! git config --get-regexp ^alias | sed -e 's/^alias\\.\\(\\w\\+\\)/\\1 =>/'
[merge]
	tool = gvimdiff
	conflictstyle = diff3
	ff = false
	branchdesc = true
[commit]
	verbose = true
[fetch]
	prune = true
[rebase]
	autosquash = true
[diff]
	colorMoved = zebra
[help]
	autocorrect = 50
