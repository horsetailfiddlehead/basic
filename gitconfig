[user]
	name = PatrickMa
	email = patrick.is@not.here
[color]
	diff = auto
	status = auto
	branch = auto
	interactive= true
	ui = true
[core]
	excludesfile = ~/.gitignore_global
	ignorecase = true
	autocrlf = input
	hookspath = ~/.githooks
[push]
	default = upstream
[credential]
	helper = /cygdrive/C/Users/Patrick/AppData/Roaming/GitCredStore/git-credential-winstore.exe
[alias]
	tree = log --graph --oneline --decorate --color
	treeall = log --graph --oneline --decorate --all --color
	last = log -1 --pretty=medium
	root = rev-parse --show-toplevel
	skipped = !git ls-files -v | grep ^[[:lower:]S]
	skip = update-index --skip-worktree
	unskip = update-index --no-skip-worktree
	hide = update-index --assume-unchanged
	unhide = update-index --no-assume-unchanged
	alias = ! git config --get-regexp ^alias | sed -e 's/^alias\\.\\([[:alnum:]_-]\\+\\)/\\1 =>/'
	rebase = rebase -i --rebase-merges
	forkpoint = !"git merge-base --fork-point \"$1\" @ #"
	show-tags = log --oneline --decorate --color --tags --no-walk
	update-rebase = pull --rebase
	mt = mergetool
	pnv = push --no-verify
	edit-description = branch --edit-description
	description = !git config --get "branch.$(git rev-parse --abbrev-ref HEAD).description"
[merge]
	tool = gvimdiff
	conflictstyle = diff3
	ff = false
	branchdesc = true
[commit]
	verbose = true
[fetch]
	prune = true
[rebase]
	autosquash = true
[diff]
	colorMoved = zebra
[help]
	autocorrect = 50
